
<% provide(:title, "PFCバランス設定") %>
<% provide(:btn_text, "設定") %>

<%= render "layouts/setting_tab" %>

<div class="form-box1">
  <div class="box-inner">
    <h5>PFC設定</h5><br>
    <%= form_with(model: @pfc, url: user_pfc_ratio_path(@user, switching: params[:switching], start_time: params[:start_time], start_date: params[:start_date]), local: true) do |f| %>
      
      <%= render partial: "form", locals: { f: f }%>
      
    <% end %>
  </div>
</div>

